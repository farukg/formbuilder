<?xml version="1.0" encoding="UTF-8"?>
<content xmlns:j="http://www.jahia.org/jahia/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0">
    <modules jcr:primaryType="jnt:modules">
        <formbuilder j:dependencies="default grid"
                   j:installedModules="Builder Form Jahia"
                   j:modulePriority="0"
                   j:moduleType="module"
                   j:title="Jahia Form Builder"
                   jcr:mixinTypes="jmix:hasExternalProviderExtension"
                   jcr:primaryType="jnt:module">
                
                <portlets jcr:primaryType="jnt:portletFolder"/>
                <files jcr:primaryType="jnt:folder"/>
                <contents jcr:primaryType="jnt:contentFolder"/>
                <templates j:rootTemplatePath="/base" jcr:primaryType="jnt:templatesFolder">
                    <files jcr:primaryType="jnt:folder"/>
                    <contents jcr:primaryType="jnt:contentFolder"/>
                    <formresponse-template j:applyOn="jnt:responsesList"
                                   j:defaultTemplate="true"
                                   j:hiddenTemplate="false"
                                   jcr:primaryType="jnt:contentTemplate">
                        <pagecontent jcr:primaryType="jnt:contentList">
                            <row column="1col16" jcr:primaryType="jnt:row"/>
                    </pagecontent>
                        <row-col1 jcr:primaryType="jnt:contentList">
                            <pager displayNbOfItemsPerPage="true"
                         displayPager="true"
                         j:bindedComponent="/modules/formbuilder/templates/formresponse-template/row-col1/main-resource-display"
                         jcr:primaryType="jnt:pager"
                         nbOfPages="10"
                         pageSize="10"/>
                            <main-resource-display jcr:primaryType="jnt:mainResourceDisplay"/>
                    </row-col1>
                </formresponse-template>
            </templates>
                
        </formbuilder>
    </modules>
</content>
